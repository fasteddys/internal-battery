@using React.AspNet;
@using UpDiddy.ViewModels
@model BrowseJobsViewModel

<div class="container">
    <div class="row">
        <div class="col-12">

            @Html.React("BrowseJobsByLocation", new { states = Model.States })
            @Html.React("BrowseJobsByCategory", new { jobCategories = Model.JobCategories })
            @Html.React("BrowseJobsByIndustry", new { industries = Model.Industries })

        </div>
    </div>
</div>
