@using UpDiddy.ViewModels.ButterCMS
@model ButterCMSBaseViewModel  

<title>@Model.Title</title>
<meta name="description" content="@Model.MetaDescription">
<meta name="keywords" content="@Model.MetaKeywords">

<meta property="og:title" content="@Model.OpenGraphTitle">
<meta property="og:description" content="@Model.OpenGraphDescription">
<meta property="og:image" content="@Model.OpenGraphImage">
<meta property="og:url" content="https://@Context.Request.Host@Context.Request.Path" />